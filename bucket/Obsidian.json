        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Stop-Process",
        "A-Remove-LinkDirectory"
    ],
    "checkver": {
        "github": "https://github.com/obsidianmd/obsidian-releases"
