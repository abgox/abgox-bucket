{
    "version": "1.98.2",
    "description": "字节跳动的一个 AI 驱动的代码 IDE。An AI-powered code IDE by ByteDance.",
    "homepage": "https://www.trae.com.cn/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.trae.com.cn/terms-of-service"
    },
    "architecture": {
        "64bit": {
            "url": "https://lf-cdn.trae.com.cn/obj/trae-com-cn/pkg/app/releases/stable/1.0.14383/win32/Trae%20CN-Setup-x64.exe",
            "hash": "17facedf44e08d328cb8bc8b71e2b936dd8b5ce1c105e42b279cdd8c40f06b15"
        }
    },
    "extract_dir": "{code_GetDestDir}",
    "innosetup": true,
    "bin": "bin/trae.cmd",
    "shortcuts": [
        [
            "Trae CN.exe",
            "Trae"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$LinkPaths = @(\"$env:AppData\\Trae CN\",\"$env:UserProfile\\.trae-cn\")",
        "A-New-LinkDirectory $LinkPaths"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'ByteDance.Trae.CN' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            }
        }
    }
}
